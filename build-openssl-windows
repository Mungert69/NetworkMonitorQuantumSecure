wget https://github.com/openssl/openssl/releases/download/openssl-3.3.2/openssl-3.3.2.tar.gz
tar xvf openssl-3.3.2.tar.gz 
cd openssl-3.3.2
sudo apt update
sudo apt install gcc-mingw-w64-x86-64 g++-mingw-w64-x86-64 binutils-mingw-w64-x86-64
./Configure mingw64 --cross-compile-prefix=x86_64-w64-mingw32- no-shared no-zlib
make


